#!/bin/sh

if [ -z "$NODE_URL" ]
then
      echo "\$NODE_URL is not defined"
      exit 1
fi

if [ -z "$AGGREGION_ACCOUNT" ]
then
      echo "\$AGGREGION_ACCOUNT is not defined"
      exit 1
fi

PERMISSION="$AGGREGION_ACCOUNT@active"

set -x
cleos -u $NODE_URL set contract $AGGREGION_ACCOUNT Aggregion/ -p $PERMISSION
